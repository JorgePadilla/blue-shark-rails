<h1 id="page-title">Register</h1>
<%= form_with(model: @user, url: register_user_path, method: :post) do |f|%>
  <div>
    <%= f.label :email%>
    <%= f.text_field :email %>
  </div>

  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

  <div>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
  </div>

  <div>
    <%= f.submit "Create Account", id: "submit-register-user" %>
  </div>
<%end%>
